@model StudentViewModel

@{
    ViewData["Title"] = "Student Details";
}

<h2>Student Detail</h2>
<table class="table tab-content">
    <tbody>
        <tr>
            <th>ID</th>
            <td>@Model.Id</td>
        </tr>
        <tr>
            <th>Student Name</th>
            <td>@Model.StudentName</td>
        </tr>

        <tr>
            <th>Age</th>
            <td>@Model.Age</td>
        </tr>
        <tr>
            <th>Address</th>
            <td>@Model.Address</td>
        </tr>
        <tr>
            <th>Jurusan</th>
            <td>@Model.Jurusan</td>
        </tr>
    </tbody>
</table>