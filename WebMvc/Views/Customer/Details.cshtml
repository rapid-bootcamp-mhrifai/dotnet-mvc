@model CustomerViewModel

@{
    ViewData["Title"] = "Customer Details";
}

<h2>customer Detail</h2>
<table class="table tab-content">
    <tbody>
        <tr>
            <th>ID</th>
            <td>@Model.Id</td>
        </tr>
        <tr>
            <th>Customer Name</th>
            <td>@Model.Name</td>
        </tr>

        <tr>
            <th>Address</th>
            <td>@Model.Address</td>
        </tr>
        <tr>
            <th>Gender</th>
            <td>@Model.Gender</td>
        </tr>
        <tr>
            <th>City</th>
            <td>@Model.City</td>
        </tr>
    </tbody>
</table>
